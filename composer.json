{
    "name": "tylerdak/stubborn",
    "description": "Generate stub files easily",
    "keywords": [
        "filesystem",
        "generate stub",
        "stub package",
        "stub"
    ],
    "homepage": "https://github.com/tylerdak/stubborn",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "voku/portable-ascii": "^2.0"
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": "1.*|2.*",
        "orchestra/testbench": "8.*|9.*"
    },
    "autoload": {
        "psr-4": {
            "Dakin\\Stubborn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "milwad-dev",
            "email": "milwad.dev@gmail.com",
            "role": "author",
            "homepage": "https://github.com/milwad-dev"
        },
        {
            "name": "tylerdak",
            "email": "tyler@dakin.one",
            "role": "author",
            "homepage": "https://github.com/tylerdak"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html ./coverage"
    }
}
